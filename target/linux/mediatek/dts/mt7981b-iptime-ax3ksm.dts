&spi0 {
	status = "okay";
	pinctrl-names = "default";
	// pinctrl-0 = <&spi0_flash_pins>;
	
	spi_nand: spi_nand@0 {
		#address-cells = <1>;
		#size-cells = <1>;
		compatible = "spi-nand";
		reg = <0>;
		spi-max-frequency = <90000000>;
		
		// spi-cal-enable;
		// spi-cal-mode = "read-data";
		// spi-cal-datalen = <7>;
		// spi-cal-data = /bits/ 8 <0x53 0x50 0x49 0x4E 0x41 0x4E 0x44>;
		// spi-cal-addrlen = <5>;
		// spi-cal-addr = /bits/ 32 <0x0 0x0 0x0 0x0 0x0>;
		
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;
		mediatek,nmbm;
		mediatek,bmt-max-ratio = <1>;
		mediatek,bmt-max-reserved-blocks = <64>;
		
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;
			
			partition@0 {
				label = "BL2";
				reg = <0x00000 0x0100000>;
				read-only;
			};
			
			partition@100000 {
				label = "u-boot-env";
				reg = <0x0100000 0x080000>;
				read-only;
			};
			
			factory: partition@180000 {
				label = "Factory";
				reg = <0x180000 0x0200000>;
				read-only;
			};
			
			partition@380000 {
				label = "FIP";
				reg = <0x380000 0x0200000>;
				read-only;
			};
			
			partition@580000 {
				label = "ubi";
				reg = <0x580000 0x06E00000>;
			};
		};
	};
};

